{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^1.2",
        "barryvdh/laravel-translation-manager": "^0.6.5",
        "filament/filament": "^3.0.52",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.8",
        "hammerstone/sidecar": "^0.4.1",
        "laravel/framework": "^10.23.1",
        "laravel/tinker": "^2.8.2",
        "livewire/livewire": "^3.0.5",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/browsershot": "^3.58.2",
        "wnx/sidecar-browsershot": "^1.9.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9.2",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23.0",
        "laravel/pint": "^1.13.1",
        "laravel/sail": "^1.25.0",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.8.1",
        "orangehill/iseed": "^3.0.3",
        "phpunit/phpunit": "^10.3.4",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
