{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "artesaos/seotools": "^1.1",
        "barryvdh/laravel-translation-manager": "^0.6.4",
        "guzzlehttp/guzzle": "^7.5.1",
        "hammerstone/sidecar": "^0.3.12",
        "laravel-notification-channels/webhook": "^2.4",
        "laravel/framework": "^9.52.7",
        "laravel/sanctum": "^3.2.4",
        "laravel/tinker": "^2.8.1",
        "livewire/livewire": "^2.12.3",
        "silviolleite/laravelpwa": "^2.0.3",
        "spatie/browsershot": "^3.57.6",
        "wnx/sidecar-browsershot": "^1.6.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.21.5",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4",
        "orangehill/iseed": "^3.0.3",
        "phpunit/phpunit": "^9.6.7",
        "spatie/laravel-ignition": "^1.6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "run-ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --no-interaction",
            "@php artisan ide-helper:eloquent"
        ],
        "run-translation": [
            "@php artisan translations:reset",
            "@php artisan translations:import",
            "@php artisan translations:find"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
